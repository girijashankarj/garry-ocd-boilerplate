{
  "name": "garry-backend-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts --exec ts-node",
    "build": "tsc",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage",
    "test:structure": "node ./scripts/verify-tests.js",
    "openapi:generate": "node ./scripts/openapi/generate-openapi.js",
    "swagger:ui": "npm run dev",
    "db:sync": "ts-node ./scripts/db/sync.ts",
    "db:seed": "ts-node ./scripts/db/seed.ts",
    "changeset": "changeset",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.8.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.0.0",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-functional": "^4.0.0",
    "eslint-plugin-immutable": "^1.0.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
