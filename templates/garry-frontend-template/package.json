{
  "name": "garry-frontend-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage",
    "test:structure": "node ./scripts/verify-tests.js",
    "prepare": "husky install",
    "changeset": "changeset",
    "changeset:version": "changeset version"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@types/lodash": "^4.14.191",
    "eslint": "^8.0.0",
    "eslint-plugin-functional": "^4.0.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-react": "^7.0.0",
    "husky": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^13.0.0",
    "lottie-react": "^2.4.1",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  }
}
